@page "{id:guid}"
@using _GenericFormModel = Project_PRN222_G5.Web.Pages.Shared.PartialViews._GenericFormModel
@model Project_PRN222_G5.Web.Areas.Admin.Pages.Users.EditModel
@{
    ViewData["Title"] = "Edit User";
    Layout = "_Layout";
}

<h1>Edit User</h1>

@await Html.PartialAsync("PartialViews/_GenericForm", new _GenericFormModel()
{
    Data = Model.Input,
    ExtraSelectLists = new Dictionary<string, List<SelectListItem>>
    {
        ["Gender"] = Model.Genders
    }
})