@page
@using Project_PRN222_G5.Web.Views.Shared.PartialViews
@model CreateModel
@{
    ViewData["Title"] = "Create User";
}

<h1>Create User</h1>

@await Html.PartialAsync("PartialViews/_GenericForm", new _GenericFormModel()
{
