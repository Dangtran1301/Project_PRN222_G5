{
  "Jwt": {
    "Key": "[your_key]",
    "Issuer": "[your_issuer]",
    "Audience": "[your_audience]"
  },

  {
    "ConnectionStrings": {
      "DefaultConnection": "Server=[localdb]\\MSSQLLocalDB;Database=[your_db_dev];Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "DetailedErrors": true
}